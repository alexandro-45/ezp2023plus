<?xml version="1.0" encoding="UTF-8"?>
<component type="desktop-application">
    <id>dev.alexandro45.ezp2023plus</id>
    <metadata_license>CC0-1.0</metadata_license>
    <project_license>GPL-2.0-or-later</project_license>
    <name>EZP2023+</name>
    <summary>Software for EZP2023+ programmer</summary>
    <description>
        <p>This is a software for a cheap Chinese programmer EZP2023+ for 24/25/93/95 flash chips.</p>
        <p>Current software features:</p>
        <ul>
            <li>Test, read and write chips</li>
            <li>View and edit dump files. Undo and redo operations are available</li>
            <li>Open and save dump files</li>
            <li>Chips database editor</li>
            <li>Chips database file format is the same as in original Chinese software</li>
        </ul>
    </description>
    <launchable type="desktop-id">dev.alexandro45.ezp2023plus.desktop</launchable>
    <screenshots>
        <screenshot type="default">
            <image>https://raw.githubusercontent.com/alexandro-45/ezp2023plus/master/data/screenshots/screenshot.png</image>
            <caption>The main window</caption>
        </screenshot>
    </screenshots>
    <url type="homepage">https://github.com/alexandro-45/ezp2023plus</url>
    <url type="bugtracker">https://github.com/alexandro-45/ezp2023plus/issues</url>
    <project_group>EZP2023plus</project_group>
    <developer id="dev.alexandro45">
        <name>Alexandro45</name>
    </developer>
    <releases>
        <release version="@BUILD_VERSION@" date="@BUILD_DATE@" type="development">
            <description>
                <p>Development build</p>
            </description>
        </release>
    </releases>
    <content_rating type="oars-1.0"/>
    <translation type="gettext">@TRANSLATION_DOMAIN@</translation>
    <recommends>
        <control>pointing</control>
        <control>keyboard</control>
        <control>touch</control>
    </recommends>
    <requires>
        <display_length compare="ge">360</display_length>
    </requires>
</component>
